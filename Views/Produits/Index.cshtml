    margin-bottom: 30px;
}

.btn_2 {
    display: inline-block;
    background-color: #4caf50;
    color: white;
    padding: 10px 20px;
    text-decoration: none;
    border-radius: 5px;
    transition: background-color 0.3s ease;
}

.btn_2:hover {
    background-color: #45a049;
}

.banner_img {
    flex: 1;
    text-align: center;
}

.banner_img img {
    max-width: 900px;
    height: 500px;
}

@@media (max-width: 768px) {
    .banner_content {
        flex-direction: column;
    }

    .banner_text, .banner_img {
        width: 100%;
        text-align: center;
    }

    .banner_text_iner {
        margin: 0 auto;
    }
}
/* Appliquer la police Poppins et un texte en gras au titre */
.banner_title {
    font-family: 'Poppins', sans-serif; /* Police Poppins */
    font-size: 3.5rem; /* Taille plus grande */
    margin-bottom: 20px;
    color: #ffffff; /* Texte blanc */
    text-transform: uppercase; /* Mettre le texte en majuscules */
    letter-spacing: 5px; /* Espacement des lettres */
    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.7); /* Ombre pour l'effet gras */
}

/* Style pour les autres éléments du texte */
.banner_description {
    font-family: 'Poppins', sans-serif; /* Appliquer la même police */
    font-weight: 400; /* Poids normal */
    font-size: 1.2rem; /* Augmenter la taille du texte de description */
    margin-bottom: 30px;
    color: rgba(0, 0, 0, 0.8); /* Texte plus doux */
}

/* Spécifier la taille de h1 à 3.5rem et s'assurer que l'espacement est correct */
.banner_text h1 {
    font-size: 3.5rem; /* Taille ajustée à 3.5rem */
    margin-bottom: 20px;
    color: #000000; /* Assurez-vous que le titre h1 est bien visible */
    font-weight: 900; /* Font-weight plus gras */
    text-transform: uppercase; /* Mettre le texte en majuscules */
    letter-spacing: 3px; /* Espacement des lettres */
