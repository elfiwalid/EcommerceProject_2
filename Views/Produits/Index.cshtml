}

.banner_content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
}

.banner_text {
    flex: 1;
    color: white;
    padding: 20px;
    text-align: left;
}

.banner_text_iner {
    max-width: 500px;
}

.banner_text h1 {
    font-size: 2.5rem;
    margin-bottom: 20px;
}

.banner_text p {
