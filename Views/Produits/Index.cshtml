    margin-top: 30px;
    width: 100%;
    max-width: 500px;
}

/* Timer Label */
.timer-label {
    display: flex;
    align-items: center;
    margin-bottom: 20px;
    font-size: 18px;
    font-weight: 600;
    color: #333;
}

.timer-label svg {
    margin-right: 10px;
    color: #1d4ed8; /* Red color for emphasis */
    transition: transform 0.3s ease;
}

.timer-label:hover svg {
    transform: scale(1.2);
}

/* Timer Blocks (Days, Hours, Minutes, Seconds) */
.timer-blocks {
    display: flex;
    justify-content: center;
    gap: 20px;
}

.timer-block {
    text-align: center;
    padding: 10px;
    background-color: #f8f8f8;
    border-radius: 8px;
    width: 60px;
    transition: background-color 0.3s ease;
}

.timer-block:hover {
    background-color: #1d4ed8;
    transform: scale(1.05);
    cursor: pointer;
}

.time-value {
    font-size: 32px;
    font-weight: 700;
    color: #333;
}
.time-value:hover {
    color:#ffffff;
}

.time-label {
    font-size: 14px;
    color: #666;
}

/* Styling for different screen sizes */
@@media (max-width: 768px) {
    .timer-section {
        width: 100%;
        padding: 15px;
        margin-top: 20px;
    }

    .timer-block {
        width: 50px;
        padding: 8px;
    }

    .time-value {
        font-size: 26px;
    }

    .timer-label {
        font-size: 16px;
    }
}

